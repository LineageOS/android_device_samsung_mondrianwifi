<!-- ===================================================================

     CM Additions

     =================================================================== -->

        <!-- usb headset -->
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="0" />
        <ctl name="MultiMedia1 Mixer AFE_PCM_TX" value="0" />
        <ctl name="AFE_PCM_RX_Voice Mixer CSVoice" value="0" />
        <ctl name="Voice_Tx Mixer AFE_PCM_TX_Voice" value="0" />
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia5" value="0" />

        <!-- afe-proxy -->
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia2" value="0" />

        <path name="deep-buffer-playback afe-proxy">
                <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="1" />
        </path>

        <path name="deep-buffer-playback usb-headphones">
                <path name="deep-buffer-playback afe-proxy" />
        </path>

        <path name="deep-buffer-playback speaker-and-usb-headphones">
                <path name="deep-buffer-playback usb-headphones" />
                <path name="deep-buffer-playback" />
        </path>

        <path name="low-latency-playback afe-proxy">
                <ctl name="AFE_PCM_RX Audio Mixer MultiMedia5" value="1" />
        </path>

        <path name="low-latency-playback usb-headphones">
                <path name="low-latency-playback afe-proxy" />
        </path>

        <path name="low-latency-playback speaker-and-usb-headphones">
                <path name="low-latency-playback usb-headphones" />
                <path name="low-latency-playback" />
        </path>

        <path name="compress-offload-playback afe-proxy">
                <ctl name="AFE_PCM_RX Audio Mixer MultiMedia4" value="1" />
        </path>

        <path name="compress-offload-playback usb-headphones">
                <path name="compress-offload-playback afe-proxy" />
        </path>

        <path name="compress-offload-playback speaker-and-usb-headphones">
                <path name="compress-offload-playback usb-headphones" />
                <path name="compress-offload-playback" />
        </path>

        <path name="usb-headphones">
        </path>

        <path name="afe-proxy">
        </path>

        <path name="speaker-and-usb-headphones">
                <path name="speaker" />
                <path name="usb-headphones" />
        </path>

        <path name="speaker-cdp">
                <path name="speaker" />
        </path>

        <path name="speaker-and-headphones-cdp">
                <path name="speaker-and-headphones" />
        </path>

        <path name="rec-high-gain-stereo-mic">
                <path name="main-mic" />
                <ctl name="ADC1 Volume" value="17" />
                <ctl name="DEC6 Volume" value="84" />
        </path>

        <!-- Compatibility with other tabpro devices -->
        <path name="rec-sub-mic">
	        <path name="rec-main-mic"/>
        </path>

        <path name="voice-call-sub-mic">
	        <path name="voice-call-main-mic"/>
        </path>

        <path name="vr-sub-mic">
	        <path name="vr-main-mic"/>
        </path>
